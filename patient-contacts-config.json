{
  "resource": "Patient",
  "context": "contact.telecom",
  "vars": [
    {
      "name": "ident",
      "expr": "Patient.identifier",
      "whenMultiple": "cross"
    },
    {
      "name": "pc",
      "expr": "Patient.contact",
      "whenMultiple": "cross"
    },
    {
      "name": "pct",
      "expr": "%pc.telecom",
      "whenMultiple": "unnest"
    }
  ],
  "filters": [
    {
      "expr": "gender = 'female'"
    }
  ],
  "columns": [
    {
      "name": "pidentifier",
      "expr": "%ident.value"
    },
    {
      "name": "cname",
      "expr": "%pc.name.text"
    },
    {
      "name": "ctel",
      "expr": "%pct.value"
    }
  ]
}
