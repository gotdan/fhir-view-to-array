{
  "resource": "Patient",
  "filters": [
    {
      "expr": "Patient.gender = 'female'"
    }
  ],
  "expand": [
    {
      "name": "pc",
      "expr": "Patient.contact"
    },
    {
      "name": "pct",
      "expr": "%pc.telecom"
    }
  ],
  "vars": [
    {
      "name": "ident",
      "expr": "Patient.identifier"
    }
  ],
  "columns": [
    {
      "name": "pidentifier",
      "expr": "%ident.system & '|' & %ident.value"
    },
    {
      "name": "cname",
      "expr": "%pc.name.text"
    },
    {
      "name": "ctel",
      "expr": "%pct.value"
    }
  ]
}

