{
  "resource": "Patient",
  "filters": [
    {
      "expr": "gender = 'female'"
    }
  ],
  "context": "Patient.contact.telecom",
  "vars": [
    {
      "name": "ident",
      "expr": "Patient.identifier"
    }
  ],
  "columns": [
    {
      "name": "pidentifier",
      "expr": "%ident.value"
    },
    {
      "name": "cname",
      "expr": "Patient.contact.name.text"
    },
    {
      "name": "ctel",
      "expr": "%context.value"
    }
  ]
}

